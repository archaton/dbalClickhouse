{
    "name": "archaton/dbal-clickhouse",
    "type": "library",
    "description": "Doctrine DBAL driver for ClickHouse",
    "keywords": [
        "doctrine",
        "dbal",
        "clickhouse",
        "driver"
    ],
    "homepage": "https://github.com/archaton/dbal-clickhouse",
    "license": "MIT",
    "authors": [
        {
            "name": "Mochalygin Aleksandr",
            "email": "a@mochalygin.ru"
        },
        {
            "name": "Mitrofanov Nikolay",
            "email": "mitrofanovnk@yandex.ru"
        },
        {
            "name": "Alexei Generalov",
            "email": "xeops123@mail.ru"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-pdo": "*",
        "ext-pcre": "*",
        "doctrine/dbal": "^2.12.1",
        "smi2/phpclickhouse": "^1.4.1",
        "doctrine/orm": "^2.8.2",
        "doctrine/doctrine-bundle": "^2.3.0",
        "symfony/cache": "^5.2.4"
    },
    "require-dev": {
        "doctrine/coding-standard": "*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "FOD\\DBALClickHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FOD\\DBALClickHouse\\Tests\\": "tests/"
        }
    }
}
